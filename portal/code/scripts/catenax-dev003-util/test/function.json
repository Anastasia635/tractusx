{
  "bindings": [
    {
      "route": "v1/test/{resource:regex(^[a-zA-Z0-9,-]{{2,80}}$)}/{id:int?}",
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post",
        "put",
        "delete"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}